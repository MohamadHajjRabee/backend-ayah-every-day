{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "index.js"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "crons": [
    {
      "path": "/updateActiveAyah",
      "schedule": "10 1 * * *"
    }
  ],
  "functions": {
    "*.js": {
      "maxDuration": 60
    }
  }
}